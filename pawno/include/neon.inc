stock AttachNeons( vehicleid, color )
{
	if ( g_HasNeons{ vehicleid } )
		RemoveNeons( vehicleid );
		
	if ( !GetVehicleModel( vehicleid ) )
		return 0;
		
	new arrModel = GetVehicleModel( vehicleid ) - 400;
	
	g_NeonObject[ vehicleid ][ 0 ] = CreateObject( color, 0.87, 0.00, -0.71,   0.00, 0.00, 0.00 );
	g_NeonObject[ vehicleid ][ 1 ] = CreateObject( color, -0.87, 0.00, -0.71,   0.00, 0.00, 0.00 );	
	
	AttachObjectToVehicle( 
		g_NeonObject[ vehicleid ][ 0 ], 
		vehicleid, 
		( VehicleData[ arrModel ][ 0 ] / 2 ) - 0.5,  
		VehicleData[ arrModel ][ 4 ] - VehicleData[ arrModel ][ 4 ],
		VehicleData[ arrModel ][ 5 ] - 0.5,
		0.00, 
		0.00, 
		0.00 );

	AttachObjectToVehicle( 
		g_NeonObject[ vehicleid ][ 1 ], 
		vehicleid, 
		( ( VehicleData[ arrModel ][ 0 ] / 2 ) + 0.5) - VehicleData[ arrModel ][ 0 ], 
		VehicleData[ arrModel ][ 4 ] - VehicleData[ arrModel ][ 4 ],
		VehicleData[ arrModel ][ 5 ] - 0.5,
		0.00, 
		0.00, 
		0.00 );
		
	g_HasNeons{ vehicleid } = true;
	return 1;
}

stock RemoveNeons( vehicleid )
{
	if ( !g_HasNeons{ vehicleid } )
		return 0;
		
	DestroyObject( g_NeonObject[ vehicleid ][ 0 ] );
	DestroyObject( g_NeonObject[ vehicleid ][ 1 ] );
	g_HasNeons{ vehicleid } = false;
	return 1;
}